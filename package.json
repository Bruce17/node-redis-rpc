{
  "name": "node-redis-rpc",
  "version": "2.0.0",
  "description": "A simple node redis rpc implementation build on top of \"node-redis-pubsub\"",
  "main": "index.js",
  "engines": {
    "node": ">=4.4.0"
  },
  "scripts": {
    "test": "mocha --recursive -R spec --require should test/**/*.spec.js --bail",
    "test-cov": "node node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha  -- -u exports --require should  --recursive test/**/*.spec.js --bail",
    "test-travis": "node node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -u exports --require should  --recursive test/**/*.spec.js --bail",
    "coveralls": "node ./node_modules/.bin/coveralls < ./coverage/lcov.info",
    "codeclimate": "node ./node_modules/.bin/codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bruce17/node-redis-rpc"
  },
  "keywords": [
    "node",
    "redis",
    "pubsub",
    "rpc"
  ],
  "author": {
    "name": "Michael Raith",
    "email": "mia87@web.de",
    "url": "https://github.com/Bruce17"
  },
  "license": "MIT",
  "dependencies": {
    "node-redis-pubsub": "^4.0.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "grunt": "^1.0.3",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-eslint": "^21.0.0",
    "grunt-mocha": "^1.1.0",
    "grunt-mocha-test": "^0.13.3",
    "grunt-remove-logging": "^0.2.0",
    "istanbul": "^0.4.5",
    "load-grunt-tasks": "^4.0.0",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0"
  }
}
